<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="using:UnderstandingTemplatedControls.ViewModels"
			 x:DataType="vm:OtherViewModel"
			 x:Class="UnderstandingTemplatedControls.Views.UserControlExample">
	<StackPanel>
		<TextBlock Text="So this is a UserControl which can hold any content"></TextBlock>
		<TextBox Text="{CompiledBinding OtherFile}" />
	</StackPanel>
</UserControl>
